<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/cardsFragment">


    <action android:id="@+id/action_global_my_collection"
        app:destination="@id/myCollectionFragment"/>

    <action android:id="@+id/action_global_cards"
        app:destination="@id/cardsFragment"/>

    <fragment
        android:id="@+id/cardsFragment"
        android:name="com.nl.professoroak.view.CardsFragment"
        android:label="fragment_cards"
        tools:layout="@layout/fragment_cards">
    </fragment>


    <fragment
        android:id="@+id/myCollectionFragment"
        android:name="com.nl.professoroak.view.MyCollectionFragment"
        android:label="fragment_my_collection"
        tools:layout="@layout/fragment_my_collection">
    </fragment>



</navigation>